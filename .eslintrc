{
  "env": {
    "es2021": true,
    "node": true,
    "browser": true
  },
  "extends": [
    "airbnb-base",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 13,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "react/prop-types": "off"
  }
}
